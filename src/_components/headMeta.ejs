<meta charset="UTF-8" />
<meta name="viewport" content="width=device-width, initial-scale=1.0" />
<meta
	name="description"
	content="<% if (locals.description) { %>
                    <% locals.description %>
                <% } else { %>
                    <% site.description %>
                <% } %> "
/>

<!-- Open Graph Meta Tags -->
<meta
	property="og:title"
	content="<%= locals.meta && locals.meta['og:title'] ? locals.meta['og:title'].content : site.meta['og:title'].content %>"
/>
<meta
	property="og:description"
	content="<%= locals.meta && locals.meta['og:description'] ? locals.meta['og:description'].content : site.meta['og:description'].content %>"
/>
<meta
	property="og:type"
	content="<%= locals.meta && locals.meta['og:type'] ? locals.meta['og:type'].content : site.meta['og:type'].content %>"
/>
<meta
	property="og:url"
	content="<%= locals.meta && locals.meta['og:url'] ? locals.meta['og:url'].content : site.meta['og:url'].content %>"
/>
<meta
	property="og:image"
	content="<%= locals.meta && locals.meta['og:image'] ? locals.meta['og:image'].content : site.meta['og:image'].content %>"
/>

<!-- Twitter Card Meta Tags -->
<meta
	name="twitter:card"
	content="<%= locals.meta && locals.meta['twitter:card'] ? locals.meta['twitter:card'].content : site.meta['twitter:card'].content %>"
/>
<meta
	name="twitter:title"
	content="<%= locals.meta && locals.meta['twitter:title'] ? locals.meta['twitter:title'].content : site.meta['twitter:title'].content %>"
/>
<meta
	name="twitter:description"
	content="<%= locals.meta && locals.meta['twitter:description'] ? locals.meta['twitter:description'].content : site.meta['twitter:description'].content %>"
/>
<meta
	name="twitter:image"
	content="<%= locals.meta && locals.meta['twitter:image'] ? locals.meta['twitter:image'].content : site.meta['twitter:image'].content %>"
/>
<meta
	name="twitter:image:alt"
	content="<%= locals.meta && locals.meta['twitter:image:alt'] ? locals.meta['twitter:image:alt'].content : site.meta['twitter:image:alt'].content %>"
/>

<link rel="icon" type="image/x-icon" href="/assets/favicon/favicon.ico" /><link
	rel="icon"
	type="image/png"
	sizes="16x16"
	href="/assets/favicon/favicon-16x16.png"
/><link
	rel="icon"
	type="image/png"
	sizes="32x32"
	href="/assets/favicon/favicon-32x32.png"
/><link
	rel="icon"
	type="image/png"
	sizes="48x48"
	href="/assets/favicon/favicon-48x48.png"
/><link rel="manifest" href="/assets/favicon/manifest.webmanifest" /><meta
	name="mobile-web-app-capable"
	content="yes"
/><meta name="theme-color" content="#171717" /><meta
	name="application-name"
	content="Jesse Demiddels portfolio"
/><link
	rel="apple-touch-icon"
	sizes="57x57"
	href="/assets/favicon/apple-touch-icon-57x57.png"
/><link
	rel="apple-touch-icon"
	sizes="76x76"
	href="/assets/favicon/apple-touch-icon-76x76.png"
/><link
	rel="apple-touch-icon"
	sizes="120x120"
	href="/assets/favicon/apple-touch-icon-120x120.png"
/><link
	rel="apple-touch-icon"
	sizes="180x180"
	href="/assets/favicon/apple-touch-icon-180x180.png"
/><meta name="apple-mobile-web-app-capable" content="yes" /><meta
	name="apple-mobile-web-app-status-bar-style"
	content="black-translucent"
/><meta name="apple-mobile-web-app-title" content="Jesse Demiddels portfolio" />
